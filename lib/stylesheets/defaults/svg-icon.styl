size-default = 22px

svg-icon
  display inline-block
  height size-default
  min-height size-default
  min-width size-default
  position relative
  width size-default

  svg
    left 0
    position absolute
    top 0

  &[icon=type-boolean]
    #left-dot
      color white
      fill white
    #right-dot
      color grey-900
      fill grey-900
