{
    "blocks": {
        "expect": false,
        "error": true
    },
    "brackets": {
        "expect": "never",
        "error": true
    },
    "colons": {
        "expect": "never",
        "error": true
    },
    "colors": {
        "expect": "always",
        "error": true
    },
    "commaSpace": {
        "expect": "always",
        "error": true
    },
    "commentSpace": {
        "expect": "always",
        "error": true
    },
    "cssLiteral": {
        "expect": "never",
        "error": true
    },
    "customProperties": {
        "expect": [],
        "error": true
    },
    "depthLimit": false,
    "duplicates": false, // we get a lot of false positives from this rule
    "efficient": {
        "expect": "always",
        "error": true
    },
    "exclude": [],
    "extendPref": {
        "expect": false,
        "error": true
    },
    "globalDupe": {
        "expect": false,
        "error": true
    },
    "groupOutputByFile": true,
    "indentPref": {
        "expect": 2, 
        "error": true 
    },
    "leadingZero": {
        "expect": "always",
        "error": true
    },
    "maxErrors": false,
    "maxWarnings": false,
    "mixed": {
        "expect": false,
        "error": true
    },
    "mixins": [],
    "namingConvention": {
        "expect": "lowercase-dash",
        "error": true
    },
    "namingConventionStrict": {
        "expect": true,
        "error": true
    },
    "none": {
        "expect": "never",
        "error": true
    },
    "noImportant": {
        "expect": true,
        "error": true
    },
    "parenSpace": {
        "expect": false,
        "error": true
    },
    "placeholders": {
        "expect": "always",
        "error": true
    },
    "prefixVarsWithDollar": {
        "expect": false,
        "error": true
    },
    "quotePref": {
        "expect": "single",
        "error": true
    },
    "semicolons": {
        "expect": "never",
        "error": true
    },
    "sortOrder": {
        "expect": "alphabetical",
        "error": true   
    },
    "stackedProperties": {
        "expect": "never",
        "error": true
    },
    "trailingWhitespace": {
        "expect": "never",
        "error": true
    },
    "universal": {
        "expect": false,
        "error": true
    },
    "valid": false,
    "zeroUnits": {
        "expect": "never",
        "error": true
    },
    "zIndexNormalize": {
        "expect": 100 ,
        "error": true
    }
}
