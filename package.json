{
  "name": "mixpanel-common",
  "main": "build/index.js",
  "version": "0.24.1",
  "description": "Mixpanel shared JS",
  "scripts": {
    "build": "node build.js && cp -r build/components . && cp -r build/report . && cp -r build/util .",
    "build-examples": "npm run build && webpack --context examples --config examples/webpack.config.js",
    "watch": "concurrently 'node build.js -w' 'webpack --context examples --config examples/webpack.config.js -w'",
    "start": "concurrently 'node build.js -w' 'webpack-dev-server --context examples --config examples/webpack.config.js --colors --inline'",
    "lint": "eslint lib",
    "prepublish": "npm run build",
    "test": "webpack --context test --config test/webpack.config.js; DISPLAY=:30 wct --plugin local",
    "unit-test": "mocha --require babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mixpanel/mixpanel-common.git"
  },
  "keywords": [
    "web components",
    "components",
    "styles"
  ],
  "author": "dev@mixpanel.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mixpanel/mixpanel-common/issues"
  },
  "homepage": "https://github.com/mixpanel/mixpanel-common",
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "autoprefixer-stylus": "^0.11.0",
    "babel-cli": "6.9.0",
    "babel-core": "6.9.1",
    "babel-loader": "6.2.4",
    "babel-plugin-import-glob": "^1.0.1",
    "babel-plugin-inline-import": "^2.0.4",
    "babel-plugin-stylus-compiler": "1.4.0",
    "babel-plugin-virtual-jade": "1.2.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "concurrently": "^3.1.0",
    "css-loader": "0.23.1",
    "eslint": "2.11.1",
    "expect.js": "0.3.1",
    "glob": "^7.1.1",
    "js-beautify": "1.6.3",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^3.1.2",
    "precommit-hook-eslint": "3.0.0",
    "raw-loader": "^0.5.1",
    "style-loader": "0.13.1",
    "stylus": "0.54.5",
    "stylus-loader": "2.1.0",
    "virtual-jade": "0.5.0",
    "virtual-jade-loader": "0.5.0",
    "watch": "^1.0.1",
    "web-component-tester": "5.0.0",
    "webcomponents.js": "0.7.22",
    "webpack": "1.13.1",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "mixpanel-browser": "2.9.16",
    "panel": "0.8.4",
    "rollbar-browser": "1.9.1",
    "virtual-dom": "2.1.1",
    "webcomponent": "^0.1.2"
  },
  "pre-commit": [
    "lint"
  ]
}
